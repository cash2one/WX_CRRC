/*TabBar样式  -  开始*/

.tabbar {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  border-top: 1px solid #dedede;
  padding: 20rpx 40rpx 10rpx 40rpx;
  height: 100rpx;
  background-color: white;
}

.t-item {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.t-icon {
  width: 50rpx;
  height: 50rpx;
}

.new {
  position: absolute;
  left: 43.5%;
  width: 96rpx;
  height: 96rpx;
}

.t-text {
  color: #848484;
}

.t-text.active {
  color: #c70019;
}

/*TabBar样式  -  结束*/

/*列表样式  -  开始*/

.list {
  margin-bottom: 128rpx;
}

.f-item {
  display: flex;
  padding: 40rpx 30rpx;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.f-item text {
  flex: 1;
  margin-left: 20rpx;
}

.f-item view {
  background-color: #c70019;
  color: white;
  text-align: center;
  line-height: 50rpx;
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
  font-size: 25rpx;
}

.f-item image {
  width: 50rpx;
  height: 50rpx;
}

/*列表样式  -  结束*/
